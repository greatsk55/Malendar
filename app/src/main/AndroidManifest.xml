<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.malendar">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- Read Permission -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- Write Permission -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconmonstr_calendar"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewImageActivity"></activity>
    </application>

</manifest>